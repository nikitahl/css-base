  "dependencies": {
    "main.css": "2.0.0",
    "main.js": "0.0.1"
  },
  "devDependencies": {
    "cssnano": "4.1.10",
    "gh-pages": "2.1.1",
    "standard": "^14.3.1",
    "parcel-plugin-meta-images": "github:plumdumpling/parcel-plugin-meta-images"
  }
}
